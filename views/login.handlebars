<div>

  <form>
    <h2>Sign Up</h2>

    <div>
      <label for="fistNameSignup">First Name:</label>
      <input type="text" id="firstNameSignup" />
    </div>
    <div>
      <label for="lastNameSignup">Last Name:</label>
      <input type="text" id="lastNameSignup" />
    </div>
    <div>
      <label for="emailSignup">Email:</label>
      <input type="text" id="emailSignup" />
    </div>
    <div>
      <label for="passwordSignup">Password:</label>
      <input type="password" id="passwordSignup" />
    </div>
    <div>
      <button id="signupButton" type="submit">Sign Up</button>
    </div>
  </form>

  <form>
    <h2>Sign In</h2>
    <div>
      <label for="emailSignin">Email:</label>
      <input type="text" id="emailSignin" />
    </div>
    <div>
      <label for="passwordSignin">Password:</label>
      <input type="password" id="passwordSignin" />
    </div>
    <div>
      <button id="signinButton" type="submit">Sign In</button>
    </div>
  </form>

</div>

<script src="./js/login.js"></script>